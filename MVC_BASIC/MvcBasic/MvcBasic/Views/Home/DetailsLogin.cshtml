@model IEnumerable<MvcBasic.Models.Login>


<table class="table table-striped table-hover">
    <thead>
        <tr>
            <th>Login Id</th>
            <th>UserName</th>
            <th>Password</th>
        
        </tr>
    </thead>
    @foreach (var item in Model)
    {


        <tbody>
            <tr>


                <td>@item.Id</td>
                <td>@item.UserName</td>
                <td>@item.Password</td>
               


            </tr>

        </tbody>
    }
</table>
