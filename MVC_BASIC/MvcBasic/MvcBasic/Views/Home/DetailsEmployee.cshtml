@model IEnumerable< MvcBasic.Models.Employee>


<table class="table table-striped table-hover">
    <thead>
        <tr>
         
            <th>Employee Id</th>
            <th>Name</th>
            <th>Age</th>
            <th>Gender</th>
            <th>Phone</th>
            <th>City</th>
        </tr>
    </thead>
    @foreach (var item in Model)
    {


        <tbody>
            <tr>
              

                <td>@item.Id</td>
                <td>@item.Name</td>
                <td>@item.Age</td>
                <td>@item.Gender</td>
                <td>@item.Phone</td>
                <td>@item.City</td>

               
            </tr>

        </tbody>
    }
</table>
