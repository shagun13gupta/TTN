

@model IEnumerable<MvcAdvance.Models.Employee>



@{
  
    ViewBag.Title = "Employee List ";
    

}

<h5><a href="/Employee/Create" style="color:blue"> Create New </a></h5>




        <table class="table table-striped table-hover">
            <thead>
                <tr>
                   
                    <th>Employee Id</th>
                    <th>Name</th>
                    <th>Age</th>
                    <th>Gender</th>
                    <th>Phone</th>
                    <th>City</th>
                </tr>
            </thead>
            @foreach (var item in Model)
            {


                <tbody>
                    <tr>
                     

                        <td>@item.Id</td>
                        <td>@item.Name</td>
                        <td>@item.Age</td>
                        <td>@item.Gender</td>
                        <td>@item.Phone</td>
                        <td>@item.City</td>

                    </tr>

                </tbody>
            }
        </table>


        
  